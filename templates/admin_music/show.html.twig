{% extends 'base-backend.html.twig' %}

{% block title %}Music{% endblock %}

{% block body %}
    <h1>Music</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ music.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ music.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ music.description }}</td>
            </tr>
            <tr>
                <th>Author</th>
                <td>{{ music.author }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ music.imageName }}</td>
            </tr>
            <tr>
                <th>AudioFile</th>
                <td>{{ music.audioFile }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ music.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_music_index') }}">back to list</a>

    <a href="{{ path('app_admin_music_edit', {'id': music.id}) }}">edit</a>

    {{ include('admin_music/_delete_form.html.twig') }}
{% endblock %}
